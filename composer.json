        "Celysium\\Permission\\PermissionServiceProvider"
      ]
    }
  },
  "authors": [
    {
      "name": "Ehsan Tavakoli",
      "email": "ehsantavakoli1995@gmail.com"
    },
    {
      "name" : "Nasser Hekmati",
      "email" : "hekmati.nasser@gmail.com"
    }
  ],
  "require": {
    "php" : "^8.0",
    "laravel/framework" : "^9.0|^10.0",
    "celysium/responser": "^1.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
